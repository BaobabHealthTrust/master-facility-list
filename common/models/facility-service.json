{
  "name": "FacilityService",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "service_id": {
      "type": "number",
      "required": true
    },
    "facility_id": {
      "type": "number",
      "required": true
    },
    "client_id": {
      "type": "number",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "service": {
      "type": "belongsTo",
      "model": "Service",
      "foreignKey": "service_id"
    },
    "facility": {
      "type": "belongsTo",
      "model": "Facility",
      "foreignKey": "facility_id"
    },
    "client": {
      "type": "belongsTo",
      "model": "Client",
      "foreignKey": "client_id"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
